{
    "autosu": false,
    "backup": true,
    "clipboard": "terminal",
    "colorscheme": "default",
    "cursorline": true,
    "diffgutter": true,
    "eofnewline": true,
    "fastdirty": false,
    "hlsearch": true,
    "hltaberrors": true,
    "incsearch": true,
    "ignorecase": true,
    "indentchar": " ",
    "keepautoindent": false,
    "mouse": true,
    "parsecursor": false,
    "relativeruler": false,
    "rmtrailingws": true,
    "ruler": true,
    "savecursor": true,
    "saveundo": false,
    "scrollmargin": 3,
    "scrollspeed": 2,
    "smartpaste": true,
    "softwrap": true,
    "splitbottom": true,
    "splitright": true,
    "statusformatl": "$(filename) $(modified)($(line),$(col)) $(status.paste)| ft:$(opt:filetype) | $(opt:fileformat)",
    "statusformatr": "$(bind:ToggleKeyMenu): help",
    "syntax": true,
    "tabmovement": false,
    "tabsize": 4,
    "tabstospaces": true,
    "useprimary": true,
    "wordwrap": false
}